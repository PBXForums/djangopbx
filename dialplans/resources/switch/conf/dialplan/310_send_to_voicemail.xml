<context name="{v_context}">
	<extension name="send_to_voicemail" number="*99[ext]" continue="false" app_uuid="006d5dab-e0c6-4352-8f06-e7986ee7b0e4" order="310">
		<condition field="destination_number" expression="^\*99(\d{2,10})$">
			<action application="answer"/>
			<action application="sleep" data="1000"/>
			<action application="voicemail" data="default ${domain_name} $1"/>
		</condition>
	</extension>
</context>
